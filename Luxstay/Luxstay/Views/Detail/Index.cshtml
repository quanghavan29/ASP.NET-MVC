
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <title>Title of the document</title>
</head>

<body>
    <div class="card-body">
        <svg style="margin-top: -30px; margin-left: -20px; color: #F65E39;"
             xmlns="http://www.w3.org/2000/svg" width="18" height="18" fill="currentColor" class="bi bi-lightning-fill" viewBox="0 0 16 16">
            <path d="M5.52.359A.5.5 0 0 1 6 0h4a.5.5 0 0 1 .474.658L8.694 6H12.5a.5.5 0 0 1 .395.807l-7 9a.5.5 0 0 1-.873-.454L6.823 9.5H3.5a.5.5 0 0 1-.48-.641l2.5-8.5z" />
        </svg><p style="margin-top: -38px; font-weight: bold;">*@Model.home.home_name*</p>
    </div>
    <section>
        <div class="container" style="margin-top: -20px; margin-bottom: -20px;">
            <div class="row">
                @foreach (var image in Model.imagesDetails)
                {
                    <div class="col-lg-4" style="padding-left: 5px; padding-right: 5px;">
                        <img src="~/Assets/images/product/@image.image_url"
                             style="height: 300px; border-radius: 10px; width: 440px; height: 290px;" class="img-fluid">
                    </div>
                }
            </div>
        </div>
        <div class="container">
            <div class="row" style="padding-top: 50px;">
                <div class="col-lg-9" style="border-right: 1px dotted;">
                    <div>
                        <p style="margin-bottom: 15px; font-weight: bold; margin-top: -15px;">@Model.home.short_description</p>
                        <div>
                            <i class="fas fa-building" style="color: gray; padding-right: 3px;"></i>
                            <span class="text-style-bold" style="font-weight: bold; color: gray;">@Model.home.home_type</span>
                        </div>
                        <div>
                            <i class="fas fa-map-marker-alt" style="color: gray; padding-right: 5px;">
                            </i> <span class="text-style-bold" style="font-weight: bold; color: gray;">@Model.home.address</span>
                        </div>
                    </div>

                    <div style="margin-top: 20px;">
                        @MvcHtmlString.Create(Model.home.detail_description.Replace(Environment.NewLine, "<br />"))
                    </div>


                    <div style="margin-top: 40px;">
                        <h5>Tiện nghi chỗ ở</h5>
                        <p style="margin-bottom: 20px">Giới thiệu về các tiện nghi và dịch vụ tại nơi lưu trú</p>

                        <div class="row">
                            <div class="col-lg-3">
                                <img src="data:image/svg+xml;base64," style="height: 25px ; margin-right: 5px" /> Wifi
                            </div>
                            <div class="col-lg-3">
                                <img src="data:image/svg+xml;base64," style="height: 25px ; margin-right: 5px" />TV
                            </div>
                            <div class="col-lg-3">
                                <img src="data:image/svg+xml;base64," style="height: 25px ; margin-right: 5px" /> Điều hoà
                            </div>
                        </div>
                        <div class="row" style="margin-top: 15px">
                            <div class="col-lg-3">
                                <img src="data:image/svg+xml;base64," style="height: 25px ; margin-right: 5px" /> BBQ
                            </div>
                            <div class="col-lg-3">
                                <img src="data:image/svg+xml;base64," style="height: 25px ; margin-right: 5px" /> Máy Giặt
                            </div>
                            <div class="col-lg-3">
                                <img src="data:image/svg+xml;base64," style="height: 25px ; margin-right: 5px" /> Dầu gội
                            </div>
                        </div>
                        <div class="row" style="margin-top: 15px">
                            <div class="col-lg-3">
                                <img src="data:image/svg+xml;base64," style="height: 25px ; margin-right: 5px" /> Tủ lạnh
                            </div>
                            <div class="col-lg-3">
                                <img src="data:image/svg+xml;base64," style="height: 25px ; margin-right: 5px" /> Máy sấy
                            </div>
                            <div class="col-lg-3">
                                <img src="data:image/svg+xml;base64," style="height: 25px ; margin-right: 5px" /> Xà phòng
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-lg-3" style="display:flex ; justify-content: flex-end; cursor: pointer;">
                    <div class="card" style="width: 18rem; height: 260px">
                        <div class="card-body">
                            <form action="Booking" method="post">
                                @{
                                    int price = Model.home.price;
                                    int million = price / 1000000;
                                    int hundred = price % 1000000 / 1000;

                                    if (million > 0)
                                    {
                                        if (hundred > 0)
                                        {
                                            <p class="price_and_address" style="padding-left: 15px; padding-top: 5px; margin-bottom: 5px;">@million,@hundred,000đ/đêm</p>
                                        }
                                        else
                                        {
                                            <p class="price_and_address" style="padding-left: 15px; padding-top: 5px; margin-bottom: 5px;">@million,000,000đ/đêm</p>
                                        }
                                    }
                                    else
                                    {
                                        <p class="price_and_address" style="padding-left: 15px; padding-top: 5px; margin-bottom: 5px;">@hundred,000đ/đêm</p>
                                    }
                                }

                                <label for="exampleInputPassword1" style="font-weight: bold; font-size: small; color: gray;">Từ Ngày</label>
                                <input type="date" class="form-control" id="exampleInputPassword1" style="margin-bottom: 5px;" name="date-checkIn" />
                                <label for="exampleInputPassword1" style="font-weight: bold; font-size: small; color: gray;">Đến Ngày</label>
                                <input type="date" class="form-control" id="exampleInputPassword1" style="margin-bottom: 5px;" name="date-checkOut" />
                                <input type="text" class="form-control" id="exampleInputPassword1" style="display: none" name="id" value="${room.getId()}" />
                                @*                                <span style="color: green;">Đặt phòng thành công!</span>*@
                                @if (Session["user"] == null)
                                {
                                    <a href="../Login" class="btn " style="background-color: #f65e38; color: white; margin-top: 15px;">Đăng nhập để đặt phòng</a>
                                }
                                else
                                {
                                    <input type="submit" class="btn " style="background-color: #f65e38; color: white; margin-top: 15px;" value="Đặt Phòng" />
                                }
                            </form>
                        </div>
                    </div>
                </div>

            </div>

        </div>
    </section>
</body>

</html>



